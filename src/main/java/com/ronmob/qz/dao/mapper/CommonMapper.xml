<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ronmob.qz.dao.CommonMapper">
	<resultMap id="SurveyClassMap" type="com.ronmob.qz.model.SurveyClass">
		<id column="id" property="id" jdbcType="INTEGER" />
		<id column="class_name" property="class_name" jdbcType="VARCHAR" />
	</resultMap>

	<!-- 获取问卷列表 -->
	<select id="getSurveyClasses" resultMap="SurveyClassMap"
		parameterType="java.lang.Integer">
		select * from survey_class where 1=1
		<if test="_parameter != -1">
			and enabled = #{enabled}
		</if>
	</select>

</mapper>